<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="application.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings >
    <!--TC默认个数 -->
    <add key="TcCount" value="55"/>
    <add key="ToolFilePath" value="C:\\Program Files\\VMTools\\teachertools\\File\\JcPaint.exe"/>
    <add key="SetPasswordPath" value="C:\\Program Files\\VMTools\\teachertools\\File\\JcChangeStuPwd.exe"/>
    <add key="USBRecovery" value="C:\\Program Files\\Jcloud\\vditools\\devredir\\checksocket.exe"/>
    <!--程序启动以后，初始化等待时间，单位为毫秒 -->
    <add key="InitializeWaitTime" value="8000"/>
    <!--教师示范等待时间，单位为毫秒 -->
    <add key="TeaShowWaitTime" value="6000"/>
    <!--学生示范等待时间，单位为毫秒 -->
    <add key="StuShowWaitTime" value="6000"/>
    <!--自动重连等待时间，单位为毫秒 -->
    <add key="ReconnectWaitTime" value="1000"/>
    <!--自动重连次数-->
    <add key="ReconnectCount" value="5"/>
    <!--Vnc Color Depth, value 0 [full], 1[256], 2[64], 3[8], default value is 1-->
    <add key="ColorDepth" value="1"/>
    <!--管理地址-->
    <add key="ManageUrl" value="../JCloudTool/ServerIp.ini"/>
    <add key="HomeworkDir" value="./ChatFileConfig.ini"/>
    <!--教师鼓励语-->
    <add key="Praise" value="你真棒！"/>
    <!--教师机关机倒计时(s)-->
    <add key="ShutdownTiemout" value="5"/>
    <add key="StuName" value="false"/>
    <add key="StuNum" value="true"/>
    <add key="StuSeatNum" value="false"/>
    <add key="ImageDir" value="./Image/"/>
    <add key="keepaliveEnable" value="true"/>
    <add key="keepaliveTime" value="1000"/>
    <add key="keepaliveInterval" value="300"/>
  </appSettings>
</configuration>